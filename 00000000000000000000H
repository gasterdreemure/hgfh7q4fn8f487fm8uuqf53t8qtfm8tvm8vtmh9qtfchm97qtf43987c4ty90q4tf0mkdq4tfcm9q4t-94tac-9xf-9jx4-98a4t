-- Gui to Lua
-- Version: 3.2

-- Instances:

local GuiHider = Instance.new("ScreenGui")
local Hider = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local Hiderr = Instance.new("TextButton")
local UnHider = Instance.new("TextButton")
local BlockHub = game.CoreGui.BlockHub

--Properties:

GuiHider.Name = "Gui Hider"
GuiHider.Parent = game.CoreGui
GuiHider.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
GuiHider.ResetOnSpawn = false
GuiHider.DisplayOrder = 999999998

Hider.Name = "Hider"
Hider.Parent = GuiHider
Hider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Hider.BackgroundTransparency = 1.000
Hider.Position = UDim2.new(0.955757558, 0, 0.978922725, 0)
Hider.Size = UDim2.new(0, 73, 0, 18)

TextLabel.Parent = Hider
TextLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
TextLabel.BackgroundTransparency = 0.200
TextLabel.Position = UDim2.new(0, 0, -0.665011048, 0)
TextLabel.Size = UDim2.new(0, 73, 0, 11)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "Showing"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 14.000

Hiderr.Name = "Hider"
Hiderr.Parent = TextLabel
Hiderr.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Hiderr.BackgroundTransparency = 0.400
Hiderr.Position = UDim2.new(0, 0, 1.06142151, 0)
Hiderr.Size = UDim2.new(0, 73, 0, 20)
Hiderr.Font = Enum.Font.SourceSans
Hiderr.Text = "Hide Gui"
Hiderr.TextColor3 = Color3.fromRGB(0, 0, 0)
Hiderr.TextSize = 14.000
Hiderr.MouseButton1Click:Connect(function()
UnHider.Visible = true
Hiderr.Visible = false
BlockHub.Enabled = false
BlockHub.TheForceGui.Enabled = false
BlockHub.BlockTP.Enabled = false
BlockHub.Grav.Enabled = false
BlockHub.GyroUp.Enabled = false
BlockHub.Spining.Enabled = false
BlockHub.Boost.Enabled = false
BlockHub["One Way"].Enabled = false
BlockHub.Speed.Enabled = false
BlockHub.Anchorer.Enabled = false
TextLabel.Text = "Hiden"
end)

UnHider.Name = "UnHider"
UnHider.Parent = TextLabel
UnHider.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
UnHider.BackgroundTransparency = 0.400
UnHider.Position = UDim2.new(0, 0, 1.06142151, 0)
UnHider.Size = UDim2.new(0, 73, 0, 20)
UnHider.Visible = false
UnHider.Font = Enum.Font.SourceSans
UnHider.Text = "Hide Gui"
UnHider.TextColor3 = Color3.fromRGB(0, 0, 0)
UnHider.TextSize = 14.000
UnHider.MouseButton1Click:Connect(function()
UnHider.Visible = false
Hiderr.Visible = true
BlockHub.Enabled = true
BlockHub.TheForceGui.Enabled = true
BlockHub.BlockTP.Enabled = true
BlockHub.Grav.Enabled = true
BlockHub.GyroUp.Enabled = true
BlockHub.Spining.Enabled = true
BlockHub.Boost.Enabled = true
BlockHub["One Way"].Enabled = true
BlockHub.Speed.Enabled = true
BlockHub.Anchorer.Enabled = true
TextLabel.Text = "UnHiden"
wait "0.8"
TextLabel.Text = "Showing"
end)
