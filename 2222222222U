-- Gui to Lua
-- Version: 3.2

-- Instances:

local TheForceGui = Instance.new("ScreenGui")
local TFG = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local Move = Instance.new("Frame")
local Player = Instance.new("TextBox")
local Mover = Instance.new("TextButton")
local Remover = Instance.new("TextButton")
local Luancher = Instance.new("TextButton")

--Properties:

TheForceGui.Name = "TheForceGui"
TheForceGui.Parent = game.CoreGui.BlockHub
TheForceGui.ResetOnSpawn = false

TFG.Name = "TFG"
TFG.Parent = TheForceGui
TFG.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TFG.BackgroundTransparency = 1.000
TFG.Position = UDim2.new(0.361201316, 0, 5.80691619e-08, 0)
TFG.Size = UDim2.new(0, 152, 0, 100)

Title.Name = "Title"
Title.Parent = TFG
Title.Active = true
Title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Title.Draggable = true
Title.Position = UDim2.new(-0.399999917, 0, 0.126999646, 0)
Title.Selectable = true
Title.Size = UDim2.new(0, 127, 0, 22)
Title.Font = Enum.Font.SourceSans
Title.Text = "Block Mover"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 14.000

Move.Name = "Move"
Move.Parent = Title
Move.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Move.Position = UDim2.new(-0.000900148414, 0, 0.980999947, 0)
Move.Size = UDim2.new(0, 127, 0, 106)

Player.Name = "Player"
Player.Parent = Move
Player.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Player.Position = UDim2.new(0.0236220472, 0, 0.0476190485, 0)
Player.Size = UDim2.new(0, 121, 0, 26)
Player.Font = Enum.Font.SourceSans
Player.Text = "Short Name here"
Player.TextColor3 = Color3.fromRGB(0, 0, 0)
Player.TextSize = 14.000
Player.TextWrapped = true
Player.FocusLost:connect(function()
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(Player.Text))) == string.lower(Player.Text) then
Player.Text = v.Name
end
end
end)

Mover.Name = "Mover"
Mover.Parent = Move
Mover.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Mover.Position = UDim2.new(0.0314960629, 0, 0.338086873, 0)
Mover.Size = UDim2.new(0, 120, 0, 20)
Mover.Font = Enum.Font.SourceSans
Mover.Text = "Use The Force!"
Mover.TextColor3 = Color3.fromRGB(255, 255, 255)
Mover.TextSize = 14.000
Mover.MouseButton1Click:Connect(function()

for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		instance:Destroy()
	end
end

wait "0.001"

local savepos = game:GetService("Players")[Player.Text].Character.HumanoidRootPart.Position

spawn(function()
while true do
game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
game:GetService("RunService").Stepped:wait()
end
end)
local function zeroGrav(part)
   if part:FindFirstChild("BodyPosition") then return end
   local temp = Instance.new("BodyPosition")
   temp.Position = savepos
   temp.Parent = part
end

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("Part") and v.Anchored == false then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(v)
       end
   end
end

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("Part") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("MeshPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("TrussPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)

wait "0.005"

for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		local Partt = instance.Parent
		instance.MaxForce = Partt:GetMass() * Vector3.new(40000,100000,40000)
		instance.D = 2500
		instance.P = 32000
		instance.Name = "Movemet"
	end
end

wait"0.1"
print "Moving Blocks"

end)

Remover.Name = "Remover"
Remover.Parent = Move
Remover.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Remover.Position = UDim2.new(0.0314960629, 0, 0.768758237, 0)
Remover.Size = UDim2.new(0, 120, 0, 20)
Remover.Font = Enum.Font.SourceSans
Remover.Text = "Remove (Buggy)"
Remover.TextColor3 = Color3.fromRGB(255, 255, 255)
Remover.TextSize = 14.000
Remover.MouseButton1Click:Connect(function()

for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance.Parent.CanCollide = true
	end
end
wait "0.05"
for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(workspace:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(game.Players:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
	if instance:IsA("BodyPosition") then
		instance:Destroy()
	end
end

for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(workspace:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(game.Players:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance:Destroy()
	end
end
wait "0.05"
for _, instance in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance:Destroy()
	end
end

print "Removed!"

end)

Luancher.Name = "Luancher"
Luancher.Parent = Move
Luancher.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Luancher.Position = UDim2.new(0.0314960629, 0, 0.526766121, 0)
Luancher.Size = UDim2.new(0, 120, 0, 20)
Luancher.Font = Enum.Font.SourceSans
Luancher.Text = "Luanch Them!"
Luancher.TextColor3 = Color3.fromRGB(255, 255, 255)
Luancher.TextSize = 14.000
Luancher.MouseButton1Click:Connect(function()

for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("RocketPropulsion") then
		instance:Destroy()
	end
end

wait "0.1"

local targetname = Player.Text
local arget = game:GetService("Players")[targetname]
local save = arget.Character.HumanoidRootPart
local PLRS = game.Players.LocalPlayer

spawn(function()
while true do
game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
game:GetService("RunService").Stepped:wait()
end
end)
local function zeroGrav(part)
   if part:FindFirstChild("RocketPropulsion") then return end
   local temp = Instance.new("RocketPropulsion")
   temp.Target = save
   temp.MaxSpeed = part:GetMass() *1000
   temp.MaxThrust = part:GetMass() *4000
   temp.ThrustD = 0.05
   temp.ThrustP = part:GetMass() *1000
   temp.Parent = part
   temp:fire()
   temp.Parent.CanCollide = false
end

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("Part") and v.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(v)
       end
   end
end
end

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("Part") and part.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(part)
       end
    end
end
end)

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("MeshPart") and v.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(v)
       end
   end
end
end

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("MeshPart") and part.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(part)
       end
   end
end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("TrussPart") and part.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(part)
       end
   end
end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("WedgePart") and part.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(part)
       end
   end
end
end)

wait"0.1"
print "Luanching!"

end)
