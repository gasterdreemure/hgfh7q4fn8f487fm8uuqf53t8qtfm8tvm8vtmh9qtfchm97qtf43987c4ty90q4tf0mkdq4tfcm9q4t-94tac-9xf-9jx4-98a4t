wait "0.1"
local StealthMode = true

local Indicator

if not StealthMode then
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    print("NOCLIP: Press Q to Activate")
    Indicator = Instance.new("TextLabel", ScreenGui)
    Indicator.AnchorPoint = Vector2.new(0, 1)
    Indicator.Position = UDim2.new(0, 0, 1, 0)
    Indicator.Size = UDim2.new(0, 200, 0, 50)
    Indicator.BackgroundTransparency = 1
    Indicator.TextScaled = true
    Indicator.TextStrokeTransparency = 0
    Indicator.TextColor3 = Color3.new(0, 0, 0)
    Indicator.TextStrokeColor3 = Color3.new(1, 1, 1)
    Indicator.Text = "Noclip: Enabled"
end

local noclip = false
local character = player.Character or player.CharacterAdded:Wait()

local mouse = player:GetMouse()

mouse.KeyDown:Connect(function(key)
    if key == "p" then
        noclip = not noclip

        if not StealthMode then
            Indicator.Text = "Noclip: " .. (noclip and "Enabled" or "Disabled")
        end
    end
end)

while true do
    player = game.Players.LocalPlayer
    character = player.Character

    if noclip then
        for _, v in pairs(character:GetDescendants()) do
            pcall(function()
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end)
        end
    end

    game:GetService("RunService").Stepped:wait()
end
wait "0.3"

local PLLL = game.Players.LocalPlayer.Character
for _, instance in pairs(PL:GetDescendants()) do
	if instance:IsA("BodyForce") then
		instance:Destroy()
	end
end

wait "0.01"

game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge*1.5
sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", "99999999999999")
game:GetService("RunService").Stepped:wait()

local function zeroGrav(part)
   if part:FindFirstChild("BodyForce") then return end
   local temp = Instance.new("BodyForce")
   temp.Force = part:GetMass() * Vector3.new(0,OGravity.Text*-1,0)
   temp.Parent = part
end

for i,v in ipairs(PLLL:GetDescendants()) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
           zeroGrav(v)
        
   end
end

PLLL.DescendantAdded:Connect(function(part)
   if part:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and part.Anchored == false then
          zeroGrav(part)
   end
end)

wait "0.2"

workspace.Gravity = OneWay.Text

wait "0.1"

for _, instance in pairs(PLLL:GetDescendants()) do
	if instance:IsA("BodyForce") then
		instance:Destroy()
	end
end

wait "0.01"
game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge*1.5
sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", "99999999999999")
game:GetService("RunService").Stepped:wait()

local NetworkAccesss = coroutine.create(function()
    spawn(function()
    while true do
    settings().Physics.AllowSleep = false
    game:GetService("Players").LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge*1.5
    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", "99999999999999")
    end
    end)
    coroutine.resume(NetworkAccesss)
end)

wait"0.01"

local PL = game.Players.LocalPlayer.Character
for _, instance in pairs(PL:GetDescendants()) do
	if instance:IsA("BodyForce") then
		instance:Destroy()
	end
end
wait"0.01"
workspace.Gravity = OGravity.Text

for _, iinstance in pairs(game.Players:GetPlayers()) do
        if(iinstance ~= PL) then
            spawn(function()
            while true do
            game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge*1.5
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", "99999999999999")
            iinstance.MaximumSimulationRadius = "-math.pow(-math.huge,-math.huge)*-math.huge*2"
            sethiddenproperty(iinstance, "SimulationRadius", "-99999999999999")
            game:GetService("RunService").Stepped:wait()
            end
        end)
    end
end
wait ()
