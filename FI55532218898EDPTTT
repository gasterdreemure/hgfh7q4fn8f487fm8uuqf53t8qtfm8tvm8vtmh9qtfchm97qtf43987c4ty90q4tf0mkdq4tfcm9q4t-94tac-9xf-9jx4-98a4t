local function zeroGrav(part)
   if part:FindFirstChild("BodyVelocity") then return end
   local temp = Instance.new("BodyVelocity")
   temp.Parent = part
   temp.MaxForce = Vector3.new(5000,part:GetMass()*2000*0.8,5000)
   temp.Velocity = part:GetMass()*Vector3.new(math.random(-40,40),-50*0.8,math.random(-40,40))
end

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(v)
       end
   end
end
for i = 1, 70 do
wait (0.07)
for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("BodyVelocity") then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then 
           v.Velocity = v.Parent:GetMass()*Vector3.new(math.random(-40,40),-140*0.8,math.random(-40,40))
       end
   end
end
wait (0.07)
for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("BodyVelocity") then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then 
           v.MaxForce = Vector3.new(8000,v.Parent:GetMass()*1000*0.8,8000)
           v.Velocity = v.Parent:GetMass()*Vector3.new(math.random(-40,40),100*0.8,math.random(-40,40))
       end
   end
end
end
wait (0)
for _, instance in pairs(game:GetDescendants()) do
	if instance:IsA("BodyVelocity") then
        if not (instance:IsDescendantOf(game.Players.LocalPlayer.Character)) then
		   instance:Destroy()
        end
	end
end
print "Finnished EarthQuake!"

end)

MiscMover4.Name = "MiscMover4"
MiscMover4.Parent = S12
MiscMover4.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
