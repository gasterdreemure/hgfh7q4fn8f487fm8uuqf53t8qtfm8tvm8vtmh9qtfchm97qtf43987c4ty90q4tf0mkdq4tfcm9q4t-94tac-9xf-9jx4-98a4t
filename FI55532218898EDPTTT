RemoverBlocks.TextSize = 14.000
RemoverBlocks.MouseButton1Click:Connect(function()
local PLRS = game.Players.LocalPlayer
local check = game.workspace:GetDescendants()
local BlockNumberCheck = game.workspace.FallenPartsDestroyHeight
for i = 1, 2 do
       spawn(function()
       while true do
       game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
       game.Players.LocalPlayer.SimulationRadius = INFIN
       game:GetService("RunService").Stepped:wait()
       end
       end)
       for i, v in ipairs(check) do
        if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
        if not (v:IsDescendantOf(PLRS.Character)) then if(v.Name ~= "HumanoidRootPart")and(v.Name ~= "LeftArm")and(v.Name ~= "RightArm")and(v.Name ~= "Tip")and(v.Name ~= "Weld")and(v.Name ~= "light")and(v.Name ~= "Ammo")and(v.Name ~= "Body")and(v.Name ~= "Center")and(v.Name ~= "Taser")and(v.Name ~= "Handcuffs")and(v.Name ~= "UpperTorso")and(v.Name ~= "LowerTorso")and(v.Name ~= "LeftFoot")and(v.Name ~= "RightFoot")and(v.Name ~= "LeftHand")and(v.Name ~= "RightHand")and(v.Name ~= "RightUpperArm")and(v.Name ~= "LeftLowerArm")and(v.Name ~= "RightLowerArm")and(v.Name ~= "RightUpperLeg")and(v.Name ~= "LeftUpperLeg")and(v.Name ~= "LeftLowerLeg")and(v.Name ~= "RightLowerLeg")and(v.Name ~= "HumanoidRootPart")and(v.Name ~= "Torso")and(v.Name ~= "Right Leg")and(v.Name ~= "Right Arm")and(v.Name ~= "Left Arm")and(v.Name ~= "Left Leg")then
 				v.CFrame = CFrame.new(v.CFrame.X,BlockNumberCheck+15,v.CFrame.Z)
                v.Velocity = Vector3.new(0,-100,0)
			end
	 	end
       end
    end
end
end)

