   local temp = Instance.new("BodyAngularVelocity")
   temp.Parent = part
   temp.MaxTorque = part:GetMass() * Vector3.new(4000,4000,4000)
   temp.P = 2000
   temp.Name = "Spin"
   temp.AngularVelocity = Vector3.new(SpinX.Text,SpinY.Text,SpinZ.Text)
end

