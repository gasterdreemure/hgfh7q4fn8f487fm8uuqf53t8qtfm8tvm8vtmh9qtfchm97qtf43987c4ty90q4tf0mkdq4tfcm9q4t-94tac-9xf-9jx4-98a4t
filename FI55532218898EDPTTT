BlackHoleNoClip.Name = "BlackHoleNoClip"
BlackHoleNoClip.Parent = S11
BlackHoleNoClip.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
BlackHoleNoClip.Position = UDim2.new(-1.87399995, 0, -5.80000019, 0)
BlackHoleNoClip.Size = UDim2.new(0, 140, 0, 15)
BlackHoleNoClip.Font = Enum.Font.SourceSans
BlackHoleNoClip.Text = "Spawn BlackHole (NoClip)"
BlackHoleNoClip.TextColor3 = Color3.fromRGB(255, 255, 255)
BlackHoleNoClip.TextSize = 14.000
BlackHoleNoClip.MouseButton1Click:Connect(function()
local targetname = BlackHolePlayerName.Text
local arget = game:GetService("Players")[targetname]
local tor = arget.Character.HumanoidRootPart
local PLRS = game.Players.LocalPlayer
local attackrange = BlackHolePower.Text

local function BlackHole(part)
   local temp = Instance.new("RocketPropulsion")
   temp.Name = "BlackHoleForce"
   temp.Parent = part
   temp.Target = tor
   temp.MaxSpeed = part:GetMass() *222
   temp.MaxThrust = part:GetMass() *888
   temp.ThrustD = 0.055
   temp.ThrustP = part:GetMass() *222
   temp:fire()
   temp.Parent.CanCollide = false
end

local near = coroutine.resume(coroutine.create(function()
game:GetService("RunService").RenderStepped:Connect(function()
	local region = Region3.new(
	  Vector3.new(tor.Position.X - tor.Size.X/2 - attackrange, tor.Position.Y - tor.Size.Y/2 - attackrange, tor.Position.Z - tor.Size.Z/2 - attackrange),
	  Vector3.new(tor.Position.X + tor.Size.X/2 + attackrange, tor.Position.Y + tor.Size.Y/2 + attackrange, tor.Position.Z + tor.Size.Z/2 + attackrange)
	)
for i,v in ipairs(workspace:FindPartsInRegion3(region)) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not v:FindFirstChild("BlackHoleForce") then
           BlackHole(v)
       end
   end
end
end
end)
end))

print "BlackHole Created"

end)

