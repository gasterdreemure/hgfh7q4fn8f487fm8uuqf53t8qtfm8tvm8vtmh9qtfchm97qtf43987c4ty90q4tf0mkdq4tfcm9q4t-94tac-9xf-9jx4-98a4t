local arget = game:GetService("Players")[targetname]
local check = game.workspace:GetDescendants()
for i = 1, 1 do
       spawn(function()
       while true do
       game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
       game.Players.LocalPlayer.SimulationRadius = INFIN
       game:GetService("RunService").Stepped:wait()
       end
       end)
       for i, v in ipairs(check) do
        if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
            if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then if (v.Name == "Torso") then
                v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 7, 0)
              v.Velocity = Vector3.new(0,1,0)
			end
	 	  end
        end
      end
    end
  end
end)

