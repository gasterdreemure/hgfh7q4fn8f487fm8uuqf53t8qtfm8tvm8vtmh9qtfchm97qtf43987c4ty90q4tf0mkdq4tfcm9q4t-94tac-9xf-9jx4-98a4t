local function Oof(iinstance)
        if(iinstance ~= PL) then
            game:GetService("RunService").RenderStepped:Connect(function()
            iinstance.MaximumSimulationRadius = "-math.pow(-math.huge,-math.huge)*-math.huge*2"
            sethiddenproperty(iinstance, "SimulationRadius", "-99999999999999")
			wait()
            end)
        end
end

for _, iinstance in pairs(game.Players:GetPlayers()) do print ""
	Oof(iinstance)
end

game.Players.PlayerAdded:Connect(function(player)
    Oof(player)
end)

wait ()

game.Players.PlayerAdded:Connect(function(player)
    Oof(player)
end)

wait ()

local function Nope(iinstance)
        if(iinstance ~= PL) then
            game:GetService("RunService").RenderStepped:Connect(function()
			game:GetService("Players").LocalPlayer.ReplicationFocus = workspace
			game.workspace.FilteringEnabled = false
			wait(1)
            end)
        end
end

for _, iinstance in pairs(game.Players:GetPlayers()) do
	Nope(iinstance)
end

game.Players.PlayerAdded:Connect(function(player)
    Nope(player)
end)

