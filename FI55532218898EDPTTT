end)

local character = game.Players.LocalPlayer.Character
    if noclipp then
        for _, v in pairs(character:GetDescendants()) do
            pcall(function()
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end)
        end
    end

wait "0.05"
print "Firing!"
game.workspace.Camera.CameraType = "Custom"
local ca = game.workspace.Camera
local cam = Vector3.new(ca.CFrame.X+-ca.Focus.X , 3, ca.CFrame.Z+-ca.Focus.Z)
local arget = game.Players.LocalPlayer
local check = game.workspace:GetDescendants()
for i = 1, 1 do
       game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
       game.Players.LocalPlayer.SimulationRadius = INFIN
       game:GetService("RunService").Stepped:wait()
       for i, v in ipairs(check) do
        if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
			if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then if(v.Name ~= "HumanoidRootPart")and(v.Name ~= "LeftArm")and(v.Name ~= "RightArm")and(v.Name ~= "Tip")and(v.Name ~= "Weld")and(v.Name ~= "light")and(v.Name ~= "Ammo")and(v.Name ~= "Body")and(v.Name ~= "Center")and(v.Name ~= "Taser")and(v.Name ~= "Handcuffs")and(v.Name ~= "UpperTorso")and(v.Name ~= "LowerTorso")and(v.Name ~= "LeftFoot")and(v.Name ~= "RightFoot")and(v.Name ~= "LeftHand")and(v.Name ~= "RightHand")and(v.Name ~= "RightUpperArm")and(v.Name ~= "LeftLowerArm")and(v.Name ~= "RightLowerArm")and(v.Name ~= "RightUpperLeg")and(v.Name ~= "LeftUpperLeg")and(v.Name ~= "LeftLowerLeg")and(v.Name ~= "RightLowerLeg")and(v.Name ~= "HumanoidRootPart")and(v.Name ~= "Torso")and(v.Name ~= "Right Leg")and(v.Name ~= "Right Arm")and(v.Name ~= "Left Arm")and(v.Name ~= "Left Leg")then
            v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
 			 v.Velocity = -cam*0.5*v:GetMass()*0.4
              v.RotVelocity = Vector3.new(0,0,0)
 			wait ""
		  end
	 	end
      end
    end
  end
end)

