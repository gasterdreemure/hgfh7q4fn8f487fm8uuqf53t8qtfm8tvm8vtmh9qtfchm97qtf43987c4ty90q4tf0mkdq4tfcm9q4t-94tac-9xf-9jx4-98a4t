local function PlayerGravity(part)
   if part:FindFirstChild("BodyForce") then return end
   local temp = Instance.new("BodyForce")
   temp.Parent = part
   temp.Force = part:GetMass() * Vector3.new(0,OGravity.Text*-1 - OneWay.Text,0)
end

local Players = game:GetService("Players")
 
local function haracterAdded(character)
   if character:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and character.Anchored == false then
          PlayerGravity(character)
      end
end

Players.LocalPlayer.CharacterAdded:Connect(haracterAdded)

