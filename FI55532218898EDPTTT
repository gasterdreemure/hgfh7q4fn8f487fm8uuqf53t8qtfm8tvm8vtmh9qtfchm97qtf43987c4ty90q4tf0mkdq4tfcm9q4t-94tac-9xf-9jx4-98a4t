local function GravChange(part)
   if part:FindFirstChild("BodyForce") then return end
   local temp = Instance.new("BodyForce")
   temp.Parent = part
   temp.Force = part:GetMass() * Vector3.new(0,OGravity.Text*-1 - OneWay.Text,0)
end

for i,v in ipairs(PLLL:GetDescendants()) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
        GravChange(v)
   end
end

wait "0.1"

workspace.Gravity = OneWay.Text

wait (0.01)

print "Changing!"

end)

