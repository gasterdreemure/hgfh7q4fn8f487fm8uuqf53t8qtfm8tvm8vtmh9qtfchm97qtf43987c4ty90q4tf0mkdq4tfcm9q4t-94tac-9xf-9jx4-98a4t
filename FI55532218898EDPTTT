   if part:FindFirstChild("BodyThrust") then return end
   local temp = Instance.new("BodyThrust")
   temp.Force = part:GetMass() * Vector3.new(GravityX_2.Text*10,GravityY_2.Text*10,GravityZ_2.Text*10)
   temp.Parent = part
end

