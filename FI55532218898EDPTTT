local PLRS = game.Players.LocalPlayer
local check = game.workspace:GetDescendants()
local BlockNumberCheck = game.workspace.FallenPartsDestroyHeight
spawn(function()
    while true do
       game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge*1.5
       sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", "99999999999999")
       game:GetService("RunService").Stepped:wait()
    end
end)
for i = 1, 2 do
    for i, v in ipairs(check) do
        if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
        if not (v:IsDescendantOf(PLRS.Character)) then
 				v.CFrame = CFrame.new(v.CFrame.X,BlockNumberCheck+20,v.CFrame.Z)
                v.Velocity = Vector3.new(0,-250,0)
			end
	 	end
    end
end
end)

