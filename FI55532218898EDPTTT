CloseForGoodButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseForGoodButton.TextSize = 14.000
CloseForGoodButton.MouseButton1Click:Connect(function()
	Well:Destroy()

wait ()

local Waiting = Instance.new("Message",workspace)
Waiting.Text = 'Good Bye'
wait(2)
Waiting:Destroy()

end)