local function zeroGrav(part)
   local temp = Instance.new("RocketPropulsion")
   temp.Parent = part
   temp.Target = save
   temp.MaxSpeed = part:GetMass() *1000
   temp.MaxThrust = part:GetMass() *4000
   temp.ThrustD = 0.05
   temp.ThrustP = part:GetMass() *1000
   temp:fire()
   temp.Parent.CanCollide = false
end

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
       if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
           zeroGrav(v)
       end
   end
end
end

wait"0.1"
print "Luanching!"

