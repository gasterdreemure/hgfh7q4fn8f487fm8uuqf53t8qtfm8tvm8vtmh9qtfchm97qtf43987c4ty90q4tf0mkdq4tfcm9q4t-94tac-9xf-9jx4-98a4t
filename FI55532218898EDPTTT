if not game:IsLoaded() then
	local Waiting = Instance.new("Message",workspace)
	Waiting.Text = 'Gravity Hub is waiting for the game to load'
	game.Loaded:Wait()
	Waiting:Destroy()
    game.workspace.FilteringEnabled = false
end
wait ()
Min = -math.pow(-math.huge,-math.huge)*-math.huge*-math.pow(-math.huge,-math.huge)*-math.huge
INFIN = math.pow(math.huge,math.huge)*math.huge*2
wait ()
-- Gui to Lua
game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
wait ()
game.Players.LocalPlayer.SimulationRadius = INFIN
wait ()
local PLRS = game.Players.LocalPlayer
local check = game.workspace:GetDescendants()
local BlockNumberCheck = game.workspace.FallenPartsDestroyHeight
local PL = game.Players.LocalPlayer
local NetworkAccess = coroutine.create(function()
    spawn(function()
    while true do
    game:GetService("RunService").Stepped:wait()
    settings().Physics.AllowSleep = false
    game:GetService("Players").LocalPlayer.MaximumSimulationRadius = INFIN
    game:GetService("Players").LocalPlayer.SimulationRadius = INFIN
    end
    end)
    coroutine.resume(NetworkAccess)
end)

for _, iinstance in pairs(game.Players:GetPlayers()) do
        if(iinstance ~= PL) then
            spawn(function()
            while true do
            game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
            game.Players.LocalPlayer.SimulationRadius = INFIN
            game.workspace.FilteringEnabled = false
            iinstance.MaximumSimulationRadius = Min
            iinstance.SimulationRadius = Min
            settings().Physics.AllowSleep = false
            game:GetService("RunService").Stepped:wait()
            end
        end)
    end
end
--             \/
local heartbeat = game:GetService("RunService").Heartbeat
spawn(function()
    while true do heartbeat:Wait()
    for i,v in pairs(game.Players:GetPlayers()) do
        if v == game.Players.LocalPlayer == false then
            game.Players.LocalPlayer.MaximumSimulationRadius = INFIN
            game.Players.LocalPlayer.SimulationRadius = INFIN
            v.MaximumSimulationRadius = Min
            v.SimulationRadius = Min
            settings().Physics.AllowSleep = false
            game:GetService("RunService").Stepped:wait()
            end
        end
    end
end)
