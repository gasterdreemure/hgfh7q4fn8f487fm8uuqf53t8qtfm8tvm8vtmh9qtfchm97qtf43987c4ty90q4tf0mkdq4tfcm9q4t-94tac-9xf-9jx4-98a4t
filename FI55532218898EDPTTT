CloseForGoodButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
CloseForGoodButton.BackgroundTransparency = 0.500
CloseForGoodButton.Position = UDim2.new(0.867924511, 0, 0, 0)
CloseForGoodButton.Size = UDim2.new(0, 21, 0, 17)
