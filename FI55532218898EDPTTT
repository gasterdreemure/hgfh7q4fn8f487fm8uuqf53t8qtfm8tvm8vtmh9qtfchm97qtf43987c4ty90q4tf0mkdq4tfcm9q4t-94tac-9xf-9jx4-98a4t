       v.Velocity = Vector3.new(v.Velocity.X,Partt:GetMass()*50,v.Velocity.Z)
      end
   end
end
wait"0.1"
print "KO!"

local function Fix(part)
   if part:FindFirstChild("BodyForce") then return end
   local temp = Instance.new("BodyForce")
   temp.Parent = part
   temp.Force = part:GetMass() * Vector3.new(GravityX.Text,workspace.Gravity*GravityY.Text,GravityZ.Text)
end

for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("BasePart" or "UnionOperation" or "Model"or "MeshPart"or "Part") and v.Anchored == false then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           Fix(v)
       end
   end
end

for _, instance in pairs(game.workspace:GetDescendants()) do
	if instance:IsA("BodyForce") then
      if not (instance:IsDescendantOf(game.Players.LocalPlayer.Character)) then
		instance:Destroy()
        end
	end
end

end)

