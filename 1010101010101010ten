-- Gui to Lua
-- Version: 3.2

-- Instances:

local Hatter = Instance.new("ScreenGui")
local HG = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local EEE = Instance.new("Frame")
local Player = Instance.new("TextBox")
local Spawn = Instance.new("TextButton")
local Remover = Instance.new("TextButton")
local HatLuanch = Instance.new("TextButton")

--Properties:

Hatter.Name = "Top Hatter"
Hatter.Parent = game.CoreGui.BlockHub
Hatter.ResetOnSpawn = false

HG.Name = "HG"
HG.Parent = Hatter
HG.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
HG.BackgroundTransparency = 1.000
HG.Position = UDim2.new(0.361201316, 0, 0.119331799, 0)
HG.Size = UDim2.new(0, 152, 0, 100)

Title.Name = "Title"
Title.Parent = HG
Title.Active = true
Title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Title.Draggable = true
Title.Position = UDim2.new(-0.399999917, 0, 0.576999664, 6)
Title.Selectable = true
Title.Size = UDim2.new(0, 127, 0, 22)
Title.Font = Enum.Font.SourceSans
