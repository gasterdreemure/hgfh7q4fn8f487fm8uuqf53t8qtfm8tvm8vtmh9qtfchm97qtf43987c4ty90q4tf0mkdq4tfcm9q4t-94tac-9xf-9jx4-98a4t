-- Gui to Lua
-- Version: 3.2

-- Instances:

local Boost = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local Stop = Instance.new("Frame")
local RotVel = Instance.new("TextButton")
local Vel = Instance.new("TextButton")
local X = Instance.new("TextBox")
local Y = Instance.new("TextBox")
local Z = Instance.new("TextBox")

--Properties:

Boost.Name = "Boost"
Boost.Parent = game.CoreGui.BlockHub
Boost.ResetOnSpawn = false

Frame.Parent = Boost
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BackgroundTransparency = 1.000
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.175151512, 0, -4.4981266e-06, 0)
Frame.Size = UDim2.new(0, 151, 0, 100)

Title.Name = "Title"
Title.Parent = Frame
Title.Active = true
Title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Title.Draggable = true
Title.Position = UDim2.new(-0.208687663, 0, 0.128067017, 0)
Title.Selectable = true
Title.Size = UDim2.new(0, 127, 0, 22)
Title.Font = Enum.Font.SourceSans
Title.Text = "Booster"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 14.000

Stop.Name = "Stop"
Stop.Parent = Title
Stop.Active = true
Stop.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Stop.Position = UDim2.new(-0.00103062904, 0, 0.984990358, 0)
Stop.Size = UDim2.new(0, 127, 0, 82)

RotVel.Name = "RotVel"
RotVel.Parent = Stop
RotVel.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
RotVel.Position = UDim2.new(0.0236220472, 0, 0.683478236, 0)
RotVel.Size = UDim2.new(0, 121, 0, 19)
RotVel.Font = Enum.Font.SourceSans
RotVel.Text = "Change RotVelocity"
RotVel.TextColor3 = Color3.fromRGB(255, 255, 255)
RotVel.TextSize = 14.000
RotVel.MouseButton1Click:Connect(function()
 
for _, instance in pairs(game:GetDescendants()) do
    if instance:IsA("BodyForce") then
        instance:Destroy()
    end
end
 
wait "0.4"
 
 
spawn(function()
while true do
game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
game:GetService("RunService").Stepped:wait()
end
end)
local function zeroGrav(part)
   part.RotVelocity = Vector3.new(X.Text,Y.Text,Z.Text)
end
 
for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("Part") and v.Anchored == false then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(v)
       end
   end
end
 
workspace.DescendantAdded:Connect(function(part)
   if part:IsA("Part") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("MeshPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)
 
workspace.DescendantAdded:Connect(function(TrussPart)
   if TrussPart:IsA("TrussPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(TrussPart)
       end
   end
end)
 
workspace.DescendantAdded:Connect(function(VehicleSeat)
   if VehicleSeat:IsA("VehicleSeat") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(VehicleSeat)
       end
   end
end)
 
wait"0.1"
print "RotVelocity Changed!"
 
end)

Vel.Name = "Vel"
Vel.Parent = Stop
Vel.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Vel.Position = UDim2.new(0.0236220472, 0, 0.380574644, 0)
Vel.Size = UDim2.new(0, 121, 0, 19)
Vel.Font = Enum.Font.SourceSans
Vel.Text = "Change Velocity"
Vel.TextColor3 = Color3.fromRGB(255, 255, 255)
Vel.TextSize = 14.000
Vel.MouseButton1Click:Connect(function()
 
for _, instance in pairs(game:GetDescendants()) do
    if instance:IsA("BodyForce") then
        instance:Destroy()
    end
end
 
wait "0.4"
 
 
spawn(function()
while true do
game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
game:GetService("RunService").Stepped:wait()
end
end)
local function zeroGrav(part)
   part.Velocity = Vector3.new(X.Text,Y.Text,Z.Text)
end
 
for i,v in ipairs(workspace:GetDescendants()) do
   if v:IsA("Part") and v.Anchored == false then
       if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(v)
       end
   end
end
 
workspace.DescendantAdded:Connect(function(part)
   if part:IsA("Part") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)

workspace.DescendantAdded:Connect(function(part)
   if part:IsA("MeshPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(part)
       end
   end
end)
 
workspace.DescendantAdded:Connect(function(TrussPart)
   if TrussPart:IsA("TrussPart") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(TrussPart)
       end
   end
end)
 
workspace.DescendantAdded:Connect(function(VehicleSeat)
   if VehicleSeat:IsA("VehicleSeat") and part.Anchored == false then
       if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
           zeroGrav(VehicleSeat)
       end
   end
end)
 
wait"0.1"
print "Velocity Changed!"
 
end)

X.Name = "X"
X.Parent = Stop
X.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
X.Position = UDim2.new(0.0238095243, 0, 0.013135612, 0)
X.Size = UDim2.new(0, 40, 0, 26)
X.Font = Enum.Font.SourceSans
X.Text = "0"
X.TextColor3 = Color3.fromRGB(0, 0, 0)
X.TextScaled = true
X.TextSize = 14.000
X.TextWrapped = true

Y.Name = "Y"
Y.Parent = Stop
Y.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Y.Position = UDim2.new(0.341269851, 0, 0.013135612, 0)
Y.Size = UDim2.new(0, 40, 0, 26)
Y.Font = Enum.Font.SourceSans
Y.Text = "100"
Y.TextColor3 = Color3.fromRGB(0, 0, 0)
Y.TextScaled = true
