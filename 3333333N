-- Gui to Lua
-- Version: 3.2

-- Instances:

local BlockTP = Instance.new("ScreenGui")
local BP = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local P = Instance.new("Frame")
local Player = Instance.new("TextBox")
local BTP = Instance.new("TextButton")
local Slow = Instance.new("TextButton")
local Shot = Instance.new("TextButton")

--Properties:

BlockTP.Name = "BlockTP"
BlockTP.Parent = game.CoreGui.BlockHub
BlockTP.ResetOnSpawn = false

BP.Name = "BP"
BP.Parent = BlockTP
BP.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
BP.BackgroundTransparency = 1.000
BP.Position = UDim2.new(0.642613351, 0, -3.12680086e-08, 0)
BP.Size = UDim2.new(0, 151, 0, 100)

Title.Name = "Title"
Title.Parent = BP
Title.Active = true
Title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Title.Draggable = true
Title.Position = UDim2.new(-0.000908404589, 0, 0.133521318, 0)
Title.Selectable = true
Title.Size = UDim2.new(0, 126, 0, 22)
Title.Font = Enum.Font.SourceSans
Title.Text = "Block Teleportation"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 14.000

P.Name = "P"
P.Parent = Title
P.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
P.Position = UDim2.new(0.166666672, -21, 1.21109211, -5)
P.Size = UDim2.new(0, 126, 0, 93)

Player.Name = "Player"
Player.Parent = P
Player.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Player.Position = UDim2.new(0.023622036, 0, 0.0240896419, 0)
Player.Size = UDim2.new(0, 121, 0, 26)
Player.Font = Enum.Font.SourceSans
Player.Text = "Player Name"
Player.TextColor3 = Color3.fromRGB(0, 0, 0)
Player.TextSize = 14.000
Player.TextWrapped = true
Player.FocusLost:connect(function()
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(Player.Text))) == string.lower(Player.Text) then
Player.Text = v.Name
end
end
end)

BTP.Name = "BTP"
BTP.Parent = P
BTP.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
BTP.Position = UDim2.new(0.023622036, 0, 0.336047232, 0)
BTP.Size = UDim2.new(0, 121, 0, 19)
BTP.Font = Enum.Font.SourceSans
BTP.Text = "Blocks Be Teleporting"
BTP.TextColor3 = Color3.fromRGB(255, 255, 255)
BTP.TextSize = 14.000
BTP.MouseButton1Click:Connect(function()

local PLRS = game.Players.LocalPlayer
local targetname = Player.Text
local arget = game:GetService("Players")[targetname]
local check = game.workspace:GetDescendants()
for i = 1, 1 do
       spawn(function()
       while true do
       game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game:GetService("RunService").Stepped:wait()
       end
       end)
       for i, v in ipairs(check) do
        if v:IsA('Part') and v.Anchored == false then
        if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
 				v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
			end
	 	end
    end
end
end
end)

Slow.Name = "Slow"
Slow.Parent = P
Slow.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Slow.Position = UDim2.new(0.0236220472, 0, 0.530733168, 0)
Slow.Size = UDim2.new(0, 121, 0, 19)
Slow.Font = Enum.Font.SourceSans
Slow.Text = "Slow Teleport"
Slow.TextColor3 = Color3.fromRGB(255, 255, 255)
Slow.TextSize = 14.000
Slow.MouseButton1Click:Connect(function()

spawn(function()
local message = Instance.new("Message",workspace)
message.Text = "Slow Teleporting Please Wait"
wait (1)
message:Destroy()
end)

local PLRS = game.Players.LocalPlayer
local targetname = Player.Text
local arget = game:GetService("Players")[targetname]
local check = game.workspace:GetDescendants()
for i = 1, 1 do
       spawn(function()
       while true do
       game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game:GetService("RunService").Stepped:wait()
       end
       end)
       for i, v in ipairs(check) do
        if v:IsA('Part') and v.Anchored == false then
        if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
 				v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
 				wait ""
			end
	 	end
    end
end
wait "0.1"
       for i, v in ipairs(check) do
        if v:IsA('MeshPart') and v.Anchored == false then
        if not (v:IsDescendantOf(PLRS.Character)) then if not (v:IsDescendantOf(arget.Character)) then
 				v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
 				wait ""
			end
	 	end
    end
end
print "Teleporting!"
end
end)

Shot.Name = "Shot"
Shot.Parent = P
Shot.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Shot.Position = UDim2.new(0.0236220472, 0, 0.735034227, 0)
Shot.Size = UDim2.new(0, 121, 0, 19)
Shot.Font = Enum.Font.SourceSans
Shot.Text = "Mini Gun Mode!"
Shot.TextColor3 = Color3.fromRGB(255, 255, 255)
Shot.TextSize = 14.000
Shot.MouseButton1Click:Connect(function()

for _, instance in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
	if instance:IsA("Part") then
		instance.CanCollide = false
	end
end
wait "0.05"
game.workspace.Camera.CameraType = "Custom"
local ca = game.workspace.Camera
local cam = Vector3.new(ca.CFrame.X+-ca.Focus.X , -10, ca.CFrame.Z+-ca.Focus.Z)
local arget = game.Players.LocalPlayer
local check = game.workspace:GetDescendants()
for i = 1, 1 do
       game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
       game:GetService("RunService").Stepped:wait()
       for i, v in ipairs(check) do
        if v:IsA('Part') and v.Anchored == false then
			if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then if(v.Name ~= "HumanoidRootPart")and(v.Name ~= "LeftArm")and(v.Name ~= "RightArm")and(v.Name ~= "Tip")and(v.Name ~= "Weld")and(v.Name ~= "light")and(v.Name ~= "Ammo")and(v.Name ~= "Body")and(v.Name ~= "Center")and(v.Name ~= "Taser")and(v.Name ~= "Handcuffs")and(v.Name ~= "Head")and(v.Name ~= "UpperTorso")and(v.Name ~= "LowerTorso")and(v.Name ~= "LeftFoot")and(v.Name ~= "RightFoot")and(v.Name ~= "LeftHand")and(v.Name ~= "RightHand")and(v.Name ~= "RightUpperArm")and(v.Name ~= "LeftLowerArm")and(v.Name ~= "RightLowerArm")and(v.Name ~= "RightUpperLeg")and(v.Name ~= "LeftUpperLeg")and(v.Name ~= "LeftLowerLeg")and(v.Name ~= "RightLowerLeg")and(v.Name ~= "Head")and(v.Name ~= "HumanoidRootPart")and(v.Name ~= "Torso")and(v.Name ~= "Right Leg")and(v.Name ~= "Right Arm")and(v.Name ~= "Left Arm")and(v.Name ~= "Left Leg")and(v.Name ~= "Handle")then
 				v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
 				v.Velocity = v:GetMass()*0.6*-cam*0.5
 				wait ""
			end
	 	end
    end
end
wait "0.1"
       for i, v in ipairs(check) do
        if v:IsA('MeshPart') and v.Anchored == false then
			if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then if(v.Name ~= "HumanoidRootPart")and(v.Name ~= "LeftArm")and(v.Name ~= "RightArm")and(v.Name ~= "Tip")and(v.Name ~= "Weld")and(v.Name ~= "light")and(v.Name ~= "Ammo")and(v.Name ~= "Body")and(v.Name ~= "Center")and(v.Name ~= "Taser")and(v.Name ~= "Handcuffs")and(v.Name ~= "Head")and(v.Name ~= "UpperTorso")and(v.Name ~= "LowerTorso")and(v.Name ~= "LeftFoot")and(v.Name ~= "RightFoot")and(v.Name ~= "LeftHand")and(v.Name ~= "RightHand")and(v.Name ~= "RightUpperArm")and(v.Name ~= "LeftLowerArm")and(v.Name ~= "RightLowerArm")and(v.Name ~= "RightUpperLeg")and(v.Name ~= "LeftUpperLeg")and(v.Name ~= "LeftLowerLeg")and(v.Name ~= "RightLowerLeg")and(v.Name ~= "Head")and(v.Name ~= "HumanoidRootPart")and(v.Name ~= "Torso")and(v.Name ~= "Right Leg")and(v.Name ~= "Right Arm")and(v.Name ~= "Left Arm")and(v.Name ~= "Left Leg")and(v.Name ~= "Handle")then
 				v.CFrame = arget.Character.HumanoidRootPart.CFrame + Vector3.new(0, 10, 0)
 				v.Velocity = v:GetMass()*-cam*0.5
 				wait ""
			end
	 	end
    end
end 
end
print "Firing!"
end)